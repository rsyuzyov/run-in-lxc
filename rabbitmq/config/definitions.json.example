{
  "rabbit_version": "3.13.0",
  "rabbitmq_version": "3.13.0",
  "product_name": "RabbitMQ",
  "product_version": "3.13.0",
  "users": [
    {
      "name": "admin",
      "password_hash": "CHANGE_ME_USE_rabbitmqctl_add_user",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ["administrator"],
      "limits": {}
    },
    {
      "name": "app_user",
      "password_hash": "CHANGE_ME",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": [],
      "limits": {}
    }
  ],
  "vhosts": [
    {
      "name": "/"
    },
    {
      "name": "production"
    },
    {
      "name": "staging"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "admin",
      "vhost": "production",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "app_user",
      "vhost": "production",
      "configure": "^app\\..*",
      "write": "^app\\..*",
      "read": "^app\\..*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "rabbit@localhost"
    }
  ],
  "policies": [
    {
      "vhost": "production",
      "name": "ha-queues",
      "pattern": "^ha\\.",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic"
      },
      "priority": 0
    },
    {
      "vhost": "production",
      "name": "quorum-queues",
      "pattern": "^quorum\\.",
      "apply-to": "queues",
      "definition": {
        "queue-mode": "quorum"
      },
      "priority": 0
    },
    {
      "vhost": "production",
      "name": "ttl-queues",
      "pattern": "^ttl\\.",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 86400000,
        "expires": 604800000
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "app.orders",
      "vhost": "production",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "app.notifications",
      "vhost": "production",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "quorum.important",
      "vhost": "production",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "quorum"
      }
    }
  ],
  "exchanges": [
    {
      "name": "app.events",
      "vhost": "production",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "app.direct",
      "vhost": "production",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "app.events",
      "vhost": "production",
      "destination": "app.orders",
      "destination_type": "queue",
      "routing_key": "order.#",
      "arguments": {}
    },
    {
      "source": "app.events",
      "vhost": "production",
      "destination": "app.notifications",
      "destination_type": "queue",
      "routing_key": "notification.#",
      "arguments": {}
    }
  ]
}

